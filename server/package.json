{
  "name": "sato",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prebuild:prod": "npm run gen-schema-types",
    "build": "webpack",
    "build:dev": "cross-env NODE_ENV=development npm run build",
    "build:prod": "cross-env NODE_ENV=production npm run build",
    "start": "node dist/main.js",
    "start:dev": "cross-env NODE_ENV=development npm run start",
    "start:prod": "cross-env NODE_ENV=production npm run start",
    "watch-webpack": "cross-env NODE_ENV=development webpack --watch",
    "watch-start": "cross-env NODE_ENV=development nodemon --exec npm run start",
    "test": "cross-env NODE_ENV=test jest",
    "lint": "tslint -c tslint.json -p tsconfig.json -t verbose",
    "gen-schema-types": "ts-node -r tsconfig-paths/register src/scripts/createTypes.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/if1live/sato.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/if1live/sato/issues"
  },
  "homepage": "https://github.com/if1live/sato#readme",
  "devDependencies": {
    "@gql2ts/types": "^1.9.0",
    "@types/debug": "0.0.31",
    "@types/express": "^4.16.0",
    "@types/faker": "^4.1.4",
    "@types/fluent-ffmpeg": "^2.1.8",
    "@types/glob": "^7.1.1",
    "@types/graphql": "^14.0.3",
    "@types/graphql-relay": "^0.4.8",
    "@types/jest": "^23.3.8",
    "@types/js-base64": "^2.3.1",
    "@types/lodash": "^4.14.117",
    "@types/mz": "0.0.32",
    "faker": "^4.1.0",
    "jest": "^23.6.0",
    "nodemon": "^1.18.5",
    "prettier": "^1.14.3",
    "ts-jest": "^23.10.4",
    "ts-loader": "^5.2.2",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.6.0",
    "tslint": "^5.11.0",
    "typescript": "^3.1.3",
    "webpack": "^4.23.1",
    "webpack-cli": "^3.1.2",
    "webpack-node-externals": "^1.7.2"
  },
  "jest": {
    "rootDir": ".",
    "testEnvironment": "node",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "moduleNameMapper": {
      "@src/(.*)": "<rootDir>/src/$1"
    }
  },
  "dependencies": {
    "@gql2ts/from-schema": "^1.10.1",
    "cross-env": "^5.2.0",
    "dataloader": "^1.4.0",
    "express": "^4.16.4",
    "fluent-ffmpeg": "^2.1.2",
    "glob": "^7.1.3",
    "graphql": "^14.0.2",
    "graphql-relay": "^0.5.5",
    "graphql-tools": "^4.0.3",
    "graphql-yoga": "^1.16.7",
    "js-base64": "^2.4.9",
    "lodash": "^4.17.11",
    "merge-graphql-schemas": "^1.5.7",
    "mz": "^2.7.0",
    "rxjs": "^6.3.3",
    "speaker": "^0.4.1",
    "ytdl-core": "^0.27.0"
  }
}
